<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="seon.startmodule.mapper.UserMapper">

    <select id="getUserList" resultType="seon.startmodule.dto.UserDTO">
        select seq, id, name, password, role, create_date, update_date
        from users
    </select>
</mapper>
